// ‚ö†Ô∏è USE AT YOUR OWN RISK
// Auto Clicker for buttons containing the text "Yes"

(function () {
    console.log("üöÄ Auto Clicker started!");

    // === CONFIGURABLE SETTING ===
    const clickInterval = 1; // Adjust interval as needed

    // === MAIN CLICK FUNCTION ===
    function autoClick() {
        const buttons = document.querySelectorAll("button");

        buttons.forEach(button => {
            if (button.textContent.trim().toLowerCase() === "yes") {
                button.click();
                console.log("‚úîÔ∏è Clicked a 'Yes' button!");
            }
        });
    }

    const clicker = setInterval(autoClick, clickInterval);

    // === STOP FUNCTION ===
    window.stopAutomation = function () {
        clearInterval(clicker);
        console.log("üõë Auto Clicker stopped.");
    };

    console.log("‚úÖ Running! To stop, type: stopAutomation()");
})();


    console.log("‚úÖ Running! To stop, type: stopAutomation()");
})();

})();
